data node{
 int val;
 node pre;
 node next;
}.


HeapPred H(node a, node b).
HeapPred HP_2(node a, node b).
HeapPred HP_1(node a, node b).


relAssume  H
 x::node<_,prev, next> * HP_1(prev,y) *  HP_2(next,y) -->
  H(x,y).

relAssume  H
 HP_2(next,y) & next!=null 
      --> H(next,y).

shape_strengthen_post [H,HP_2][].