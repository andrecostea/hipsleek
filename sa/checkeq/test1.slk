checkeq[] x = 1 == y = 1.
//VALID

//3.
checkeq[x] x = 1 == y = 1.
//FAIL

//4.
checkeq[] x = 1 == y = 2.
//FAIL

//5
checkeq[x] x = 1 == x = 2.
//FAIL

//6.
checkeq[x,y] x = 1 == y = 1.
//FAIL

//7
checkeq[] x = y == y = x.
//VALID

//8.
checkeq[x] x = y == y = x.
//FAIL

//9
checkeq[x,y] x = y == y = x.
//FAIL

//10.
checkeq[] x = y & x = 1 == y = 1 & x = y.
//VALID

//11.
checkeq[x] x = y & x = 1 == y = 1 & x = y.
//VALID

//12.
checkeq[x,y] x = y & x = 1 == y = 1 & x = y.
//VALID

