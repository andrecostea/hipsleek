data node { int val ; node next }.
data node2 { int val ; node next }.

hpred ll(node x, int n).
hpred rr(node x, int n).

checkeq [x] x::node<a,b> -|- x::node<c,d> .
// VALID --> [{a,c},{b,d}]

checkeq [x] x::node<a,b> -|- y::node<c,d> .
// FAIL

checkeq [] x::node<a,b> -|- y::node<c,d> .
// VALID --> [{a,c},{b,d},{x,y}]



checkeq [] x::ll<a> -|- y::rr<b> .
// VALID --> [{x,y},{a,b}]
