(assert (or (and (and (and (< d 1) (= dm 0)) (= dn 0)) (= Anon_36 1)) (and (and (and (and (and (<= 0 Anon_36) (<= (+ (- 0 dn) 2) Anon_36)) (<= Anon_36 dn)) (<= Anon_36 2)) (<= 1 dm)) (> d 0))))
