/* singly linked lists */

/* representation of a node */

data node {
	int val; 
	node next;	
}.


pred app5<> == self=null
        or self::node<_,q> * q::app5a<>
  inv true.

pred app5a<> == self=null 
	or self::node<_, q> * q::app5<> 
  inv true.

pred_reuse [*][*].

