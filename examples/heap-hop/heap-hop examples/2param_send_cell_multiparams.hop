
message cell [val|-> * param0 == val * param1 == val * _nextparam0 == 0 * _nextparam1 == 0]

contract C {
  initial final state start { !cell -> end; }
  final state end {}
}

fmain() [emp] {
  local e,f,x;

  x = new();
  /* [x |-> ] */
  (e,f) = open(C,x,x);

  /* [e|-> C{start}, p0:x * f|-> ~C{start}, p0:_x * x |-> ] */
  put(e,x) || get(f);

  /* [e|-> C{end}, p0:0 * f|-> ~C{end}, p0:0 ] */
  close(e,f);
} [emp]

put(e,x) [e|-> C{start}, p0:x, p1:x * x|-> ] {
  /* [e|-> C{start}, p0:x * x|-> * x == x]  */
  send(cell,e,x);
  /* [e|-> C{start}, p0:0]  */
} [e|->C{end}, p0:0, p1:0]

get(f) [f|-> ~C{start},pr:_ee, p0:_x, p1:_x1] {
  local y;

  y = receive(cell,f);
  /* [f|-> ~C{start},pr:_ee, p0:_x * y|-> * y == _x] */
  dispose(y);
} [f|-> ~C{end},pr:_ee, p0:_y, p1:_y]
