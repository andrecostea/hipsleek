/*
@M - mutable   (0)
@I - immutable (1)
@L - lending   (2)
@A - no access (3)
*/

//data cell {int val;}.

data node {
	int val; 
	node next;	
}.

pred ll<n, v1> == self = null & n = 0 
	or self::node<a@v1, q> * 
    q::ll<n-1, v1>
  inv n >= 0.

// (8) 
// Valid due to late inst
// fail with --field-imm
checkentail x::node<p,q> * q::ll<n,_> & n > 1 |- [kkk] x::ll<n+1,kkk> & n > 1.
print residue.

