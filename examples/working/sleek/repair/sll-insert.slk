data node {
   int val;
   node next
}.

pred ll<n> == self = null & n = 0
	or (exists v, q: self::node<v, q> * q::ll<n-1>).

void insert(node x, int a)
	requires x::ll<n> & n > 0 
	ensures exists k: x::ll<k> & k = n+1;.

synthesize [node x,int a]
x::node<an01,fr32> & n-1=0 & fr32=null
~>
(exists fr33,an5,q_6,an83:
q_6::node<an83,fr33> * x::node<an5,q_6> & fr33=null).
