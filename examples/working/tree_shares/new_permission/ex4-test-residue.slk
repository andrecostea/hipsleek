data cl {int val;}.
data cl2 {int val;}.
data cl3 {int val;}.

macro L == (#,) .
macro R == (,#) .
macro LL == ((#,),).
macro LR == ((,#),).
macro RL == (,(#,)).
macro RR == (,(,#)).


//(1)
checkentail x::cl<n> & n > 3 |- x::cl<m> & m > 2.
print residue.
expect Valid.

//(2)
checkentail x::cl<n> & n > 1 |- x::cl<m> & m > 2.
print residue.
expect Fail.

//(3)
checkentail x::cl<_> |- x::cl<_>.
print residue.
expect Valid.

//(4)
checkentail x::cl(@@LR)<_> |- x::cl<_>.
print residue.
expect Fail.

//(5)
checkentail x::cl(@@LR)<_> |- x::cl(@@L)<_>.
print residue.
expect Fail.

//(6)
checkentail x::cl(@@L)<_> |- x::cl(@@R)<_>.
print residue.
expect Fail.

//(7)
checkentail x::cl<_> |- x::cl(@@R)<_>.
print residue.
expect Valid.

//(8)
checkentail x::cl<_> |- x::cl(@@L)<_>.
print residue.
expect Valid.

//(9)
checkentail x::cl(@@LR)<_> |- x::cl<_>.
print residue.
expect Fail.

//(10)
checkentail x::cl(@@RR)<_> |- x::cl(@@LR)<_>.
print residue.
expect Fail.

//(11)
checkentail x::cl(v)<_> |- x::cl(v)<_>.
print residue.
expect Valid.

//(12)
checkentail x::cl(v1)<_> |- x::cl(v)<_>. 
print residue.
expect Valid.
//valid v1=v or v1>v

//(13)
checkentail x::cl(@@R)<_> |- x::cl(@@RL)<_>.
print residue.
expect Valid.
//valid x(R,R)

//(14)
checkentail x::cl(@@L)<_> * x::cl(@@R)<_>|- x::cl<_>.
expect Valid.

//(15)
checkentail x::cl(@@L)<_> * x::cl(@@R)<_>|- x::cl(v)<_>.
print residue.
expect Valid.
//valid v=full or v<full

//(16)
checkentail x::cl(@@L)<_> * x::cl(@@R)<_>|- x::cl(v)<_> & v=@@L+@@RL.
print residue.
expect Valid.
//valid v=L+(R,L)

//(17)
checkentail x::cl(@@L)<_> * x::cl(@@R)<_>|- x::cl(v)<_> &  v=@@L+@@RL.
print residue.
expect Valid.
//valid

//(18)
checkentail x::cl(@@L)<_> * x::cl(@@R)<_>|- x::cl(v)<_> &  v=@@R+@@RL.
print residue.
expect Fail.
//fail


//(19)
checkentail x::cl(v)<_> &v=@@L+@@LR |- x::cl(v)<_> & v=@@R+@@RL.
print residue.
expect Valid.
//valid (contradiction)

//(20)
checkentail x::cl(@@L)<_> * x::cl(@@R)<_>|- 1>2.
print residue.
expect Fail.

