X := {[x,y,ox,oy] : x<y | x>=y & x>1 & (ox-oy)>=0};

Y := {[x,y,ox,oy] : (not(x<y) | 1=1) & (not(x>=y & x>1) | (ox-oy)>=0) & (not(x>=y & x<=1) | 1=0)};

X subset Y;

Y subset X;

Z := {[x,y,nx,ny] : not(x>=y & x>1 & nx>=ny & nx>1 & nx=x+1 & ny=x+y) | ((x-y)-(nx-ny)>0)};

T := {[x,y,nx,ny] : 1=1};

Z subset T;

T subset Z;