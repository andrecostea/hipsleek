data node {
	int val; 
	node next;	
}.

pred ll<n> == self = null & n = 0 
	or self::node<_, q> * q::ll<n-1> 
  inv n >= 0.

checkentail @I<:@M |- false.
print residue.

checkentail @L<:@I |- false.
print residue.

checkentail v1<:v2 & v2<:v3 |- v1<:v3.
print residue.

checkentail v=@M |- v<:@I.
print residue.

checkentail v<:@M |- v=@M.
print residue.

checkentail v<:@M & v!=@M |- false.
print residue.

