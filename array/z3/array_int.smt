(declare-const x Int)
(declare-const y Int)
(declare-const a1 (Array Int Int))
(declare-const a2 (Array Int Int))
(declare-const a3 (Array Int Int))
(assert (= 1 x))
(assert (= (- x 1) y))
(assert (= (store a1 x 1) a2))
;(assert (not (= (select a2 1) 1)))
(assert (= (store a2 (+ y 1) 2) a3))
(assert (not (= (select a3 1) 2)))
(check-sat)