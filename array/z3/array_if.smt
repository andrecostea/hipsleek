(declare-const a1 (Array Int Int))
(declare-const a2 (Array Int Int))
(assert (= (store a1 0 1) a2))
(declare-const a3 (Array Int Int))
(assert (= (store a1 0 2) a3))
(assert (not (= (select a2 0) 2)))
(declare-const a4 (Array Int Int))
(assert (or (= a4 a2) (= a4 a3)))
(declare-const a Int)
(assert (= (select a4 0) a))
(assert (and (not (= a 1)) (not (= a 2))))
(check-sat)
