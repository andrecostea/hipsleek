/* TEST: Double-If
int double_if(int x)
  req HI(x) & (x=1|x=0)
  ens LO(res) & res=x
{
  int y=0,z=0; // LO(y') & LO(z') & HI(x) & FLOW(x,x') & y'=0 & z'=0 & (x=1|x=0) & x'=x

  if(x == 0) {
    y = 1;
    // LO(y_0) & LO(z') & HI(x) & FLOW(x,x') & FLOW(x',y') & y_0=0 & z'=0 & (x=1|x=0) & x'=x & x=0 & y'=1
  }

  // THEN:
