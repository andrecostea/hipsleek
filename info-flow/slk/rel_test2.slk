  }

  // THEN:
  // - PURE:
  //   (((y_0=0 & z_0=0 & (x=1|x=0) & x'=x & x'=0 & y'=1 & y'=0 & z'=1) | (y'=0 & z_0=0 & (x=1|x=0) & x'=x & x'!=0 & y'=0 & z'=1)))
  // - INFO:
  //   (((LO(y_0) & LO(z_0) & HI(x) & FLOW(x,x') & FLOW(x',y') & FLOW(y',z')) | (LO(y') & LO(z_0) & HI(x) & FLOW(x,x') & FLOW(y',z'))))
  // ELSE:
