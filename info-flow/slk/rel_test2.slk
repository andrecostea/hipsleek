  //   ((y_0=0 & z'=0 & (x=1|x=0) & x'=x & x'=0 & y'=1 & y'!=0) | (y'=0 & z'=0 & (x=1|x=0) & x'=x & x'!=0 y'!=0))
  // - INFO:
  //   (((LO(y_0) & LO(z') & HI(x) & FLOW(x,x') & FLOW(x',y')) | (LO(y') & LO(z') & HI(x) & FLOW(x,x'))))

  // MERGED:
