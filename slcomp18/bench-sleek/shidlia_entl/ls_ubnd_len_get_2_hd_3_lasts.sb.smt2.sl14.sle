
data Refnode {
	Refnode next;
}.

pred ls<y_2,n_3> ==   emp & n_3=0 & self=y_2
	or  exists u_4,k: self::Refnode<u_4:next> * u_4::ls<y_2,k> & k=n_3-1 & 0<=n_3-1
.


checkentail x::ls<y,n> & 1000<=n
	 |- (exists u,v,t,w,r,k: r::Refnode<y:next> * t::Refnode<w:next> * u::Refnode<v:next> * w::Refnode<r:next> * x::Refnode<u:next> * v::ls<t,k> & k=n-5 ) .
