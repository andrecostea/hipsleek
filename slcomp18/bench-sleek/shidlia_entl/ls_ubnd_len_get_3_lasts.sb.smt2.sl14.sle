
data Refnode {
	Refnode next;
}.

pred ls<y_2,n_3> ==   emp & n_3=0 & self=y_2
	or  exists u_4,k: self::Refnode<u_4:next> * u_4::ls<y_2,k> & k=n_3-1 & 0<=n_3-1
.


checkentail x::ls<y,n> & 1000<=n
	 |- (exists u,v,t,k: t::Refnode<y:next> * u::Refnode<v:next> * v::Refnode<t:next> * x::ls<u,k> & k=n-3 ) .
