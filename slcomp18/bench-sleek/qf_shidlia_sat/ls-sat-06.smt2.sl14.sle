
data RefSll_t {
	RefSll_t next;
	int data_slk;
}.

pred lsls<dt1,len1,out,dt2,len2> ==   emp & self=out & dt1=dt2 & len1=len2
	or  exists u,dt3,len3: self::RefSll_t<u:next,dt1:data_slk> * u::lsls<dt3,len3,out,dt2,len2> & dt1<dt3+0 & len1=len3+1
.


checkentail u_emp::lsls<d1,n1,z_emp,d2,n2> * z_emp::RefSll_t<y_emp:next,d2:data_slk> * y_emp::lsls<d3,n3,w_emp,d4,n4> & d1!=d2 & n3=n4+3
	 |- false
.
