
data RefDll_t {
	RefDll_t next;
	RefDll_t prev;
}.

pred ldll<P,len1,F,L,len2> ==   emp & self=F & P=L & len1=len2
	or  exists u,len3: self::RefDll_t<u:next,P:prev> * u::ldll<self,len3,F,L,len2> & len1=len3+1
.


checkentail E1::ldll<F1,x1,E3,F3,x3> * E2::ldll<F2,x2,E4,F4,x4> * E3::ldll<F3,x3,E4,F4,x4> * E4::ldll<F4,y4,E3,F3,y3> * E3::ldll<F3,x3,E5,F5,x5> * E5::ldll<F5,y5,E3,F3,y3> * E4::ldll<F4,x5,E6,F6,x6> & x3>x5 & E1!=E3
	 |- false
.
