
data Refnode {
	Refnode next;
	Refnode prev;
}.

pred dll_rev<p_2,tl_3,n_4> ==  self::Refnode<n_4:next,p_2:prev> & self=tl_3
	or  exists x_5: tl_3::Refnode<n_4:next,x_5:prev> * self::dll_rev<p_2,x_5,tl_3>
.

pred dllnull<p_7> ==  self::Refnode<null:next,p_7:prev>
	or  exists x_8: self::Refnode<x_8:next,p_7:prev> * x_8::dllnull<self>
.


checkentail x::dllnull<y>
	 |- (exists t: x::dll_rev<y,t,nil> ) .
