
data RefDll_t {
	RefDll_t next;
	RefDll_t prev;
}.

pred dll<bk,pr,nx> ==   emp & self=nx & bk=pr
	or  exists u: self::RefDll_t<u:next,pr:prev> * u::dll<bk,self,nx> & self!=nx & bk!=pr
.


checkentail x_emp::RefDll_t<w_emp:next,nil:prev> * w_emp::dll<y_emp,x_emp,z_emp> * y_emp::RefDll_t<z_emp:next,nil:prev>
	 |- x_emp::dll<y_emp,nil,z_emp>.
