
data RefDll_t {
	RefDll_t next;
	RefDll_t prev;
}.

pred dll<bk,pr,nx> ==   emp & self=nx & bk=pr
	or  exists u: self::RefDll_t<u:next,pr:prev> * u::dll<bk,self,nx> & self!=nx & bk!=pr
.


checkentail x_emp::RefDll_t<y_emp:next,nil:prev> * y_emp::RefDll_t<z_emp:next,x_emp:prev> & y_emp!=z_emp & x_emp!=nil & y_emp!=nil & x_emp!=y_emp
	 |- x_emp::dll<y_emp,nil,z_emp> & x_emp!=z_emp.
