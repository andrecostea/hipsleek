
data RefSL2_t {
	RefSL2_t n1;
	RefSL2_t n2;
}.

pred skl1<ex> ==   emp & self=ex
	or  exists tl: self::RefSL2_t<tl:n1,null:n2> * tl::skl1<ex> & self!=ex
.

pred skl2<ex> ==   emp & self=ex
	or  exists tl,Z1: self::RefSL2_t<Z1:n1,tl:n2> * Z1::skl1<tl> * tl::skl2<ex> & self!=ex
.


checkentail x4::skl2<x5> * x5::RefSL2_t<x6:n1,x7:n2> * x7::skl1<x6> * x1::RefSL2_t<x1_1:n1,x2:n2> * x1_1::RefSL2_t<x1_2:n1,nil:n2> * x1_2::skl1<x2> * x2::RefSL2_t<nil:n1,nil:n2>
	 |- x1::skl2<nil> * x4::skl2<x6>.
