
data RefGTyp {
	RefGTyp f0;
	RefGTyp f1;
}.

pred one<> ==   emp & null!=self
.

pred zero<> ==   emp & null=self
.

pred succ2rec<x2,y1,y2> ==  self::zero<> * y1::one<> & x2=y2
	or  x2::succ1rec<y2> * self::one<> * y1::zero<>
.

pred Q<y2> ==  self::zero<> * y2::zero<>
	or  exists x1,x2: x1::succ2rec<x2,self,y2> * x1::Q<x2>
.

pred P<x2> ==  self::one<> * x2::one<> * self::Q<x2>
.

pred succ1rec<y1> ==  self::zero<> * y1::one<>
.


checkentail x0::P<x1>
	 |- false
.
