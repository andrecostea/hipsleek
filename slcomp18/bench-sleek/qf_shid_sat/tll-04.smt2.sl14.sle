
data RefTll_t {
	RefTll_t lson;
	RefTll_t rson;
	RefTll_t next;
}.

pred tll<ll,rl> ==  self::RefTll_t<null:lson,null:rson,rl:next> & self=ll
	or  exists ls,rs,z: self::RefTll_t<ls:lson,rs:rson,null:next> * ls::tll<ll,z> * rs::tll<z,rl>
.


checkentail x0::tll<x0,x0> * x1::tll<x1,x1> * x2::tll<x2,x2> * x3::tll<x3,x3>
	 |- false
.
