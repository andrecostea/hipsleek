
data RefNLL_lvl1_t {
	RefNLL_lvl1_t next1;
}.

data RefNLL_lvl2_t {
	RefNLL_lvl2_t next2;
	RefNLL_lvl1_t down;
}.

pred sll<out> ==   emp & self=out
	or  exists u: self::RefNLL_lvl1_t<u:next1> * u::sll<out> & self!=out
.

pred nll<out> ==   emp & self=out
	or  exists h,u: self::RefNLL_lvl2_t<u:next2,h:down> * h::sll<null> * u::nll<out>
.


checkentail x0::nll<nil>
	 |- false
.
