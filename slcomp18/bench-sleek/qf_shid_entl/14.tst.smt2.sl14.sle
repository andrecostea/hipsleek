
data RefGTyp {
	RefGTyp f0;
	RefGTyp f1;
}.

pred BinListSecond<> ==  emp
	or  exists yp,xp: self::RefGTyp<yp:f0,xp:f1> * xp::BinListSecond<> & null!=self
.

pred BinTree<> ==  emp
	or  exists yp,xp: self::RefGTyp<yp:f0,xp:f1> * yp::BinTree<> * xp::BinTree<> & null!=self
.


checkentail x::BinListSecond<>
	 |- x::BinTree<>.
