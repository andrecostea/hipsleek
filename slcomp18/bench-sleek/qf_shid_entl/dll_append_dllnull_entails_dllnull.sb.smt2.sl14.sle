
data Refnode {
	Refnode next;
	Refnode prev;
}.

pred dll<p_2,tl_3,n_4> ==  self::Refnode<n_4:next,p_2:prev> & self=tl_3
	or  exists x_5: self::Refnode<x_5:next,p_2:prev> * x_5::dll<self,tl_3,n_4>
.

pred dllnull<p_7> ==  self::Refnode<null:next,p_7:prev>
	or  exists x_8: self::Refnode<x_8:next,p_7:prev> * x_8::dllnull<self>
.


checkentail x::dll<y,z,t> * t::dllnull<z>
	 |- x::dllnull<y>.
