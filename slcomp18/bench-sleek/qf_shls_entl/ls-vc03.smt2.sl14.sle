
data RefSll_t {
	RefSll_t next;
}.

pred ls<out> ==   emp & self=out
	or  exists u: self::RefSll_t<u:next> * u::ls<out> & self!=out
.


checkentail x_emp::RefSll_t<y_emp:next> * y_emp::ls<w_emp> * w_emp::ls<z_emp> * z_emp::RefSll_t<u_emp:next> * u_emp::RefSll_t<v_emp:next> * v_emp::ls<r_emp> * r_emp::RefSll_t<s_emp:next>
	 |- x_emp::ls<s_emp>.
