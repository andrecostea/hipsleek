
data RefDll_t {
	RefDll_t next;
	RefDll_t prev;
}.

pred ldll<P,len1,F,L,len2> ==   emp & self=F & P=L & len1=len2
	or  exists u,len3: self::RefDll_t<u:next,P:prev> * u::ldll<self,len3,F,L,len2> & len1=len3+1
.


checkentail E1::RefDll_t<E2:next,E1_p:prev> * E2::ldll<E2_p,x2,E3,E3_p,x3> & E1=E2_p & x1=x2+1
	 |- E1::ldll<E1_p,x1,E3,E3_p,x3>.
