data node2 {
	int val;
    int priority;
    node2 prev;
	node2 next;
}.

pred dll2<p,r,l,n> == self=r & p=l & n=0
  or self::node2<_,_,p,q> * q::dll2<self,r,l,n-1>
  inv n >= 0.

checkentail (exists z1: x::dll2<p,res,z1,j-1> * res::node2<_,_,z1,q> * q::dll2<res,r,z,m-1> & (m=n-j+1) & n>=1
& a=1) |- x::dll2<p,r,l, n> & n >= a & a >= 1.

