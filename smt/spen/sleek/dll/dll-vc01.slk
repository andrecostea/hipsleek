data pto {
     pto next;
     pto prev;
}.

pred dll<ex1,pr,hd> ==
// self=pr & hd=ex1
  self=hd & ex1=pr
     or (exists u: self::pto<u,pr> * u::dll<ex1,self,hd>).

// Fail -> expected Valid.

checkentail x_emp::pto<w_emp,null> * w_emp::pto<y_emp,x_emp> * y_emp::pto<z_emp,w_emp> |-
            x_emp::dll<y_emp,null,z_emp>.
