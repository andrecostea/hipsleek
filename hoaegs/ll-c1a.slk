data node {
	int val;
	node next
}.

relation dom(int[] a, int low, int high).

axiom dom(a,low,high) & low<=l & h<=high ==> dom(a,l,h).
//axiom dom(a,l+1,h) & dom(a,l,l+1)  ==> dom(a,l,h).
//axiom dom(a,l,h-1) & dom(a,h-1,h) ==> dom(a,l,h).
axiom l>=h ==> dom(a,l,h).
axiom dom(a,l,k) & dom(a,k,h) ==> dom(a,l,h).

pred ll2<i,j,aaa> == self = null & i=j //& dom(aaa,i,j)
  or self::node<v, r> * r::ll2<i+1,j,aaa> & aaa[i]=v
      & dom(aaa,i,i+1) 
  inv i<=j & dom(aaa,i,j).


