//1
checksat charUp("abc", 1, 'd') = "adc".

//2
checksat charUp("abc", 1, 'd') = "aec".

//3
checksat charUp("abc", 0, 'd') = "dbc".

//4
checksat s = charUp(s, (slen(s)-1), '\x00') & endzero(s).

//5
checksat s = charUp(s, (slen(s)-1), '\x00') & endzero(s) & s = "a\\0c".

//6
checkentail s = charUp(s, (slen(s)-1), '\x00') |- !(nonzero(s)).

//7
checkentail s = charUp(s, (slen(s)-1), '\x00') |- endzero(s).

//8
checkentail endzero(s) & s = charUp(s,offset,c) & offset <= slen(s) |- endzero(s).