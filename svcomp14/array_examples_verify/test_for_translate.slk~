
/* 1: Valid */
checkentail
   x'=x
   & a[5] = x
   & res=1+x'
  |-  emp& a[5]=x & res=1+x.

/* 2: Valid */
checkentail
   x'=x
   & res = 1+x
   & ((!(5=5)| a_5=x))
   |- a_5=x & res = 1+x'.


/* 3: Valid */
checkentail
  a[i] = x
  |- a[i] = x.

/* 4: This sentence seems not to make any sense without a universial quantifiers. */
checkentail
  a[i] = x
  |- a[j] = x.


/* 5: Same as above. i is free. */
checkentail
  a[i] = x
  |- a[5] = x.

/* 6: valid */
checkentail
  a[i] = x
  & i=5
  |- a[5] = x.


/* 7: Same reason with 4 & 5 */
checkentail
  a[i] = x
  & i>3
  |- a[5] = x.

/* 8: Valid */
checkentail
  a[5]=x
  |-a[5]=x.


/* 9: Valid */
checkentail
  (!(5=5)|a_5=x)
  |-a_5=x.

/* 10: Valid */
checkentail
  a[i] = 3
  & i=5
  |- a[5]>2.

/* 11: Valid */
checkentail
  a[i]=a[i]
  & i=5
  |- a[5]=a[5].

/* 12: Valid */
checkentail
  a = c
  & a[i]=b[i]
  & i=5
  |- a[5]=b[5] & a[5] = c[5].
